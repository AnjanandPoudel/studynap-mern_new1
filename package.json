{
  "name": "studynap-mern",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "restart": "sudo kill $(sudo lsof -t -i:3001 && sudo kill $(sudo lsof -t -i:3000 )",
    "server": "nodemon ./bin/www ",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server \" \"npm run client \"",
    "client-install": "npm install --prefix client"
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "^3.1.5",
    "express": "~4.16.1",
    "formidable": "^1.2.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.11.8",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.6",
    "react-router-dom": "^5.2.0",
    "save": "^2.4.0"
  }
}
